{
  "kind": "singleType",
  "collectionName": "knowledgepages",
  "info": {
    "singularName": "knowledgepage",
    "pluralName": "knowledgepages",
    "displayName": "knowledgepage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "main_section_title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "main_section_decscription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "headline": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "yaizy_image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "yaizy_title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "yaizy_description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "info_items": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "components.yaizy-info-item",
      "repeatable": true
    },
    "schools": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::school.school"
    },
    "lab_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lab-item.lab-item"
    }
  }
