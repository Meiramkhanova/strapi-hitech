{
  "kind": "collectionType",
  "collectionName": "tab_contents",
  "info": {
    "singularName": "tab-content",
    "pluralName": "tab-contents",
    "displayName": "main-activity-items"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "tab": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tab.tab",
      "inversedBy": "tab_contents"
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "desc": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "icon": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "unique": true
    },
    "sections": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "sections.tab-content-main-section",
        "sections.about-department",
        "sections.main-areas-activity",
        "sections.centers-of-department",
        "sections.school-of-tab"
      ]
    }
  }
}
